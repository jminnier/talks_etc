@article{Aschard:2012aa,
	Abstract = {Genome-wide association studies have identified hundreds of common genetic variants associated with the risk of multifactorial diseases. However, their impact on discrimination and risk prediction is limited. It has been suggested that the identification of gene-gene (G-G) and gene-environment (G-E) interactions would improve disease prediction and facilitate prevention. We conducted a simulation study to explore the potential improvement in discrimination if G-G and G-E interactions exist and are known. We used three diseases (breast cancer, type 2 diabetes, and rheumatoid arthritis) as motivating examples. We show that the inclusion of G-G and G-E interaction effects in risk-prediction models is unlikely to dramatically improve the discrimination ability of these models.},
	Author = {Aschard, H. and Chen, J. and Cornelis, M.C. and Chibnik, L.B. and Karlson, E.W. and Kraft, P.},
	Date-Added = {2014-09-29 15:34:05 +0000},
	Date-Modified = {2014-09-29 17:41:39 +0000},
	Doi = {10.1016/j.ajhg.2012.04.017},
	Journal = {American Journal Human Genetics},
	Journal-Full = {American journal of human genetics},
	Mesh = {Area Under Curve; Arthritis, Rheumatoid; Breast Neoplasms; Computer Simulation; Diabetes Mellitus, Type 2; Environment; Female; Gene-Environment Interaction; Genetic Predisposition to Disease; Genome, Human; Genome-Wide Association Study; Humans; Middle Aged; Models, Genetic; Models, Statistical; Models, Theoretical; Probability; Risk; Risk Factors},
	Number = {6},
	Pages = {962-972},
	Pmc = {PMC3370279},
	Pmid = {22633398},
	Pst = {ppublish},
	Title = {Inclusion of gene-gene and gene-environment interactions unlikely to dramatically improve risk prediction for complex diseases},
	Volume = {90},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.ajhg.2012.04.017}}

